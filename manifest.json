{
  "manifest_version": 2,
  "name": "资源扫描（定制版）",
  "version": "1.0.0",
  "description": "用于定制化扫描图片，视频资源。",
  "icons": {
    "16": "images/icon-16.png",
    "46": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-64.png",
    "default_title": "用于定制化扫描图片，视频资源。",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://detail.tmall.com/",
    "https://detail.tmall.com",
    "downloads"
  ]
}
